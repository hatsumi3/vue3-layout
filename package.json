{
  "name": "vue3-layout",
  "private": true,
  "version": "0.0.0",
  "repository": "https://github.com/hatsumi3/vue3-layout",
  "type": "module",
  "types": "src/index.ts",
  "module": "./dist/vue3-layout.js",
  "main": "./dist/vue3-layout.umd.cjs",
  "files": [
    "dist",
    "src/index.ts",
    "src/components/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue3-layout.js",
      "export": "./dist/vue3-layout.umd.cjs"
    },
    "./style": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "tsc": "vue-tsc --noEmit",
    "build": "npm run tsc && npm run test &&vite build",
    "preview": "vite preview",
    "lint": "eslint --fix src/*.{ts,vue} && eslint --fix src/**/*.{ts,vue}",
    "format": "prettier -w src/*.{ts,vue} && prettier -w src/**/*.{ts,vue}",
    "fix": "npm run format && npm run lint",
    "test": "jest",
    "test-v": "jest --verbose"
  },
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/test-utils": "^2.3.2",
    "@vue/vue3-jest": "^29.2.4",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.11.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "msw": "^1.2.1",
    "postcss-nested": "^6.0.1",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "tailwindcss-pseudo-elements": "^2.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vue-tsc": "^1.4.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
